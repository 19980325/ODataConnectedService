<UserControl x:Class="Microsoft.OData.ConnectedService.Views.AdvancedSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Microsoft.OData.ConnectedService.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="433">
    <StackPanel HorizontalAlignment="Left" Height="283" Margin="10,10,-122,0" VerticalAlignment="Top" Width="412">
        <Label x:Name="label" Content="Great! We have enough information to connect to this OData service."/>
        <TextBlock x:Name="TextBlock" TextWrapping="Wrap" Padding="5">
            <Hyperlink Name="settings" Click="settings_Click">AdvancedSettings</Hyperlink>
        </TextBlock>
    </StackPanel>
</UserControl>
